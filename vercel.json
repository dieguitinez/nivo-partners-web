{
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "www.nivopartners.com"
                }
            ],
            "destination": "https://nivopartners.com/$1",
            "permanent": true
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}